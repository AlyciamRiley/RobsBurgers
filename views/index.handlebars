<div class="container-fluid">
	<div class="row" id="header">
			{{> header }}
	</div>
	<!--HeaderRow-->
	<div class="row">
		<div class="col-md-6">
			<h2>Uneaten Burgers</h2>
		<ul>
  {{#each burgers}}
    {{#unless devoured}}
      {{> burger-block devoured=true}}
    {{/unless}}
  {{/each}}
</ul>
		</div>
		<!--UneatenColumn-->
		<div class="col-md-6">
			<h2>Devoured Burgers</h2>
			<ul>
				{{#each burgers}}
					{{#if devoured}}
					{{>burger-block devoured=false}}
					{{/if}}
					{{/each}}
			</ul>

		</div>
		<!--Uneaten Column-->
	</div>
	<!--BurgerListRow-->
	<div class="row">
		<h2>submit new burger</h2>
		<form class="create-form">
			<div class="form-group">
				<label for"ca">Burger Name:</label>
				<input type="text" id="ca" name="name">
			</div>
		</form>
		<button type="button" class="btn btn-primary" id="submit">Submit</button>
	</div>
	<!--submitRow-->



</div>